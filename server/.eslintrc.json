{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "func-names": 0,
        "max-classes-per-file": 0,
        "no-underscore-dangle": 0,
        "no-trailing-spaces": 0
    }
}
